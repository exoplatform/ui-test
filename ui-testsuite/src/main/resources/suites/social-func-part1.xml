<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Platform Sniff Test" verbose="1" preserve-order="true">
	<test name="social-func-part1" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class name="org.exoplatform.selenium.platform.social.functional.SOC_TestConfig" />
			<class name="org.exoplatform.selenium.platform.social.functional.SOC_Space_ApplicationManagement" />
			<class name="org.exoplatform.selenium.platform.social.functional.SOC_Dashboard" />
			<class name="org.exoplatform.selenium.platform.social.functional.SOC_Space_SpacesActivities" />
			<class name="org.exoplatform.selenium.platform.social.functional.SOC_Space_navigation_Management" />
			<class name="org.exoplatform.selenium.platform.social.functional.SOC_Space_MemberManagement_Leave" />
			<class name="org.exoplatform.selenium.platform.social.functional.SOC_Space_MemberManagement_Remove" />
			<class name="org.exoplatform.selenium.platform.social.functional.SOC_Space_Space_Information" />
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>