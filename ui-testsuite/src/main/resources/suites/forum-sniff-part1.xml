<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Platform Sniff Test" verbose="1" preserve-order="true">
	<test name="forum-sniff" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
		    <class name="org.exoplatform.selenium.platform.forum.sniff.Forum_TestConfig" />
		    <class name="org.exoplatform.selenium.platform.forum.sniff.Forum_Search"/>
			<class name="org.exoplatform.selenium.platform.forum.sniff.Forum_BookMark" />
			<class name="org.exoplatform.selenium.platform.forum.sniff.Forum_Category" />
			<class name="org.exoplatform.selenium.platform.forum.sniff.Forum_Forum_BasicAction" />
			<class name="org.exoplatform.selenium.platform.forum.sniff.Forum_Forum_MoreAction" />
			<class name="org.exoplatform.selenium.platform.forum.sniff.Forum_Post"/>
			<class name="org.exoplatform.selenium.platform.forum.sniff.Forum_Administration" />
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>