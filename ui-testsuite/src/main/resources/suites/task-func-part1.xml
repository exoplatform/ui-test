<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Platform Sniff Test" verbose="1" preserve-order="true">
	<test name="task-func-part1" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class name="org.exoplatform.selenium.platform.task.functional.TASK_TestConfig_1" />
			<class name="org.exoplatform.selenium.platform.task.functional.TASK_Projects_CreateProject" />
			<class name="org.exoplatform.selenium.platform.task.functional.TASK_Projects_ProjectOverview" />
			<class name="org.exoplatform.selenium.platform.task.functional.TASK_Projects_ChangeProjectColor" />
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>