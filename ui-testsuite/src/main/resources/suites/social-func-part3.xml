<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Platform Sniff Test" verbose="1" preserve-order="true">
	<test name="social-func-part3" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class name="org.exoplatform.selenium.platform.social.functional.SOC_TestConfig1" />
			<class name="org.exoplatform.selenium.platform.social.functional.SOC_Notification_Administration" />
		    <class name="org.exoplatform.selenium.platform.social.functional.SOC_Intranet_Notification_Administration" />
		    <class name="org.exoplatform.selenium.platform.social.functional.SOC_Notification_NotificationSettings" />
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>