<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Platform Sniff Test" verbose="1" preserve-order="true">
	<test name="social-sniff-part1" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_TestConfig_2" />
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_Manage_Remote_Calendar" />
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_Manage_Shared_Calendar" />
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_Manage_Color" />
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_Manage_Common_Actions" />
	     	<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_Event_Delete" />
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>