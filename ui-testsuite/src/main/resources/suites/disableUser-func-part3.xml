<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Disable User Func Test" verbose="1" parallel="none">
	<test name="disableUser-func-part3" enabled="true" preserve-order="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class name="org.exoplatform.selenium.platform.disableUser.functional.Disable_User_TestConfig" />
			<class name="org.exoplatform.selenium.platform.disableUser.functional.Disable_User_Document" />
			<class name="org.exoplatform.selenium.platform.disableUser.functional.Disable_User_Forum" />
			<class name="org.exoplatform.selenium.platform.disableUser.functional.Disable_User_Wiki" />
			<class name="org.exoplatform.selenium.platform.disableUser.functional.Disable_User_Notifications" />
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>