<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Platform Sniff Test" verbose="1" preserve-order="true">
	<test name="plf-sniff" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
		    <class name="org.exoplatform.selenium.platform.plf.sniff.Plf_TestConfig"/>
		    <class name="org.exoplatform.selenium.platform.plf.sniff.Plf_HomepageGadget_WhoIsOnlineGadget"/>
			<class name="org.exoplatform.selenium.platform.plf.sniff.Plf_HomepageGadget_InvitationGadget"/>
			<class name="org.exoplatform.selenium.platform.plf.sniff.Plf_HomepageGadget_SuggestionGadget"/>
			<class name="org.exoplatform.selenium.platform.plf.sniff.Plf_Navigation_LeftNavigation"/>
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>