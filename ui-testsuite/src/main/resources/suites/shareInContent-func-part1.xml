<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Platform Function Test" verbose="1" preserve-order="true">
	<test name="ecms-func" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
		    <class name="org.exoplatform.selenium.platform.ecms.functional.ShareInContentExplorer_AdminView" />
			<class name="org.exoplatform.selenium.platform.ecms.functional.ShareInContentExplorer_IconView" />
			<class name="org.exoplatform.selenium.platform.ecms.functional.ShareInContentExplorer_ListView" />			
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>